{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "declaration": true,
    "sourceMap": true,
    "allowUnusedLabels": true,
    "allowUnreachableCode": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "noEmit": true
  }
}
